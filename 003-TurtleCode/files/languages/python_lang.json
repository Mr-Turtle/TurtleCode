{
  "name": "Python",
  "default_file_extensions": ["py"],
  "default_run_command": "start cmd.exe @cmd /k {FILEPATH}",
  "default_compile_command": "pyinstaller {FILEPATH} --onefile",
  "single_comment": "//",
  "single_comment_col": "#ff0000",
  "single_comment_style": "italic",
  "block_comment_start": "/*",
  "block_comment_end": "*/",
  "block_comment_col": "#00ff00",
  "block_comment_style": "italic",
  "indent_chars": [
    ":","(","{","["
  ],
  "syntax_highlighting": [
    {
      "word": "def",
      "style": null,
      "col": "#ff0000"
    },
    {
      "word": "lambda",
      "style": "bold",
      "col": "#00ffff"
    }
  ],
  "autofill": [
    "def",
    "lambda"
  ]
}